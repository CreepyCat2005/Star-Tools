    <Content Include="Graphics\MadeByTheCommunity_White-00.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
    </Content>
    <Content Include="Graphics\Star-Tools-Logo-White.png">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>

    <RootNamespace>Star_Citizen_Pfusch</RootNamespace>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Background.png" />
    <None Remove="Graphics\Discord-Logo.png" />
    <None Remove="Graphics\MadeByTheCommunity_White-00.png" />
